<?xml version="1.0" encoding="UTF-8" ?>
<list xmlns="http://tzswivl.com/xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://tzswivl.com/xml schema.xsd">
{% for news in newsEntitySet %}
    <item id="{{ news.id }}" date="{{ news.createdAt|date('d.m.Y H:i') }}">
        <announce><![CDATA[{{ news.excerpt }}]]></announce>
        <description><![CDATA[{{ news.fullText }}]]></description>
    </item>
{% endfor %}
</list>